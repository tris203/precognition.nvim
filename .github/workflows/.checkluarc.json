{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "runtime.version": "LuaJIT",
  "runtime.path": [
    "lua/?.lua",
    "lua/?/init.lua"
  ],
  "Lua.workspace.library": [
    "/github/workspace/deps/neodev.nvim/types/stable"
    "${3rd}/busted/library"
